generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Student {
  id Int @id
  name String 

  visits Visit[]
}

model Visit {
  id Int @id @default(autoincrement())
  timeIn DateTime 
  timeOut DateTime?
  duration Int?

  studentId Int

  student Student @relation(fields: [studentId], references: [id])
}